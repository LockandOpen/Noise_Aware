{
  "manifest_version": 3,
  "name": "Edge Impulse Audio Classifier",
  "version": "1.0",
  "description": "classify model and audjust the sound",
  "permissions": ["tabs",
  "activeTab",
  "storage"],
  "action": {
    "default_popup": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["edge-impulse-standalone.wasm","edge-impulse-standalone.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "host_permissions": ["<all_urls>"]
}